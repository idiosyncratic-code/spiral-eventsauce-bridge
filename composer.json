{
  "name": "idiosyncratic/spiral-eventsauce-bridge",
  "type": "library",
  "license": "EUPL-1.2",
  "description": "Cycle Database Implementation for Eventsauce",
  "homepage": "https://github.com/idiosyncratic-code/spiral-eventsauce-bridge",
  "support": {
    "source": "https://github.com/idiosyncratic-code/spiral-eventsauce-bridge"
  },
  "require": {
    "php": ">=8.4",
    "eventsauce/eventsauce": "^3.7",
    "eventsauce/id-encoding": "^1.0",
    "eventsauce/uuid-encoding": "^1.0",
    "ramsey/uuid": "^4.9"
  },
  "autoload": {
    "psr-4": {
      "Idiosyncratic\\Spiral\\EventSauceBridge\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "scripts": {
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "idiosyncratic/coding-standard": "^3.0",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpstan/phpstan": "^1.10",
    "qossmic/deptrac-shim": "^1.0"
  }
}
